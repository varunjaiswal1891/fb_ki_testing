package thread;

class demo2 extends Thread
{
	public void run()
	{
	  for(int i=0;i<10;i++){
		  System.out.print(i+" ");
	  }
	}
	
}



 class demo {

	public static void main(String[] args) {
		demo2 obj= new demo2();
		Thread t= new Thread(obj);
		t.start();
            System.out.println("hii");
	}

}
