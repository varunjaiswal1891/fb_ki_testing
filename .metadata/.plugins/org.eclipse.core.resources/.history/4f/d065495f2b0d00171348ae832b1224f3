package thread;

import java.io.IOException;

class demo2 extends Thread
{
	public void run()
	{
	  for(int i=0;i<10;i++){
		  System.out.print(i+" ");
	  }
	  System.out.println("hii");
	}
	 
	
}



 class demo {

	public static void main(String[] args) throws IOException {
		demo2 obj= new demo2();
Thread t= new Thread(obj);
               System.in.read();
              t.start();
            System.out.println("hii");
	}

}
